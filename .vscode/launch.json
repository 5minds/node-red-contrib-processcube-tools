{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to Node-RED",
        "address": "localhost",
        "port": 9229,
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/package_src"
      },
      {
      "type": "node",
      "request": "launch",
      "name": "Debug npm test",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test",
        "--"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}